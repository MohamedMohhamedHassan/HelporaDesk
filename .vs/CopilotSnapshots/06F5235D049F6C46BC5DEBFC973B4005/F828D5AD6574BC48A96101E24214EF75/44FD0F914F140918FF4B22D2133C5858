using System.ComponentModel.DataAnnotations;

namespace ServiceCore.Models
{
    public class Project
    {
        public int Id { get; set; }
        [Required]
        [StringLength(200)]
        public string? Name { get; set; }

        [StringLength(50)]
        public string? Status { get; set; }

        [StringLength(100)]
        public string? Lead { get; set; }
    }
}
